{
  "name": "wnb-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js\"",
    "debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r ts-node/register/transpile-only -r tsconfig-paths/register --inspect dist/server.js\"",
    "clean": "rm -rf dist deploy",
    "build": "tsc -p tsconfig.json",
    "serve": "node dist/server.js 10001",
    "serve-debugger": "node --inspect dist/server.js 10001",
    "seq:migrate": "node_modules/.bin/sequelize-ts db:migrate",
    "seq": "node_modules/.bin/sequelize-ts",
    "ts-compile": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@jdpnielsen/http-error": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-azure-ad-oauth2": "0.0.4",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "sequelize": "^6.3.5",
    "soap": "^0.36.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@jdpnielsen/sequelize-cli-typescript": "^6.3.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
